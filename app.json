{
  "expo": {
    "name": "audix-app",
    "slug": "audix-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "@/images/icons/AudixLogoNavySimple.png",
    "scheme": "audixapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "@/images/icons/AudixLogoNavySimple.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "@/images/icons/AudixLogoNavySimple.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "@/images/icons/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
